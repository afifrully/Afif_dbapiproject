<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.afif_dbapiproject">
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Afif_dbapiproject">
        <activity android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
            <action android:name="android.intent.action.MAIN" />
            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
        </activity>
    </activity>
    <activity
        android:name="com.example.afif_dbapiproject.FormHandphone"
        android:label="@string/title_activity_add_new" >
    </activity>
    <activity
    android:name="com.example.afif_dbapiproject.DetailHandphone"
    android:label="@string/title_activity_detail_handphone" >
    </activity>
</manifest>

